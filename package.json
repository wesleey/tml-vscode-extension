{
  "name": "tml-extension",
  "displayName": "TML",
  "description": "Turing Machine Language Syntax",
  "publisher": "wesley-barbosa",
  "version": "0.0.0",
  "engines": {
    "vscode": "^1.99.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wesleey/tml-vscode-extension.git"
  },
  "icon": "icon.png",
  "categories": ["Programming Languages"],
  "contributes": {
    "languages": [{
      "id": "tml",
      "aliases": ["Turing Machine Language", "tml"],
      "extensions": [".tml"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "tml",
      "scopeName": "source.tml",
      "path": "./syntaxes/tml.tmLanguage.json"
    }],
    "iconThemes": [{
      "id": "tml-icons",
      "label": "TML Icons",
      "path": "./icon-theme.json"
    }]
  }
}
